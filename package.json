{
  "name": "ecij",
  "version": "0.2.0",
  "description": "Rolldown and Vite plugin to Extract CSS-in-JS",
  "keywords": [
    "css-in-js"
  ],
  "homepage": "https://github.com/nstepien/ecij#readme",
  "bugs": {
    "url": "https://github.com/nstepien/ecij/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nstepien/ecij.git"
  },
  "license": "MIT",
  "author": "Nicolas Stepien",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./plugin": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "index.js",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "build": "rolldown -c",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@rolldown/pluginutils": "^1.0.0-beta.51",
    "oxc-parser": "^0.98.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.13",
    "prettier": "^3.6.2",
    "rolldown": "^1.0.0-beta.51",
    "rolldown-plugin-dts": "^0.18.0",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@^7.2.7",
    "vitest": "^4.0.13"
  },
  "overrides": {
    "vite": "$vite"
  }
}
